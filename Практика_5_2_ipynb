        "N_DOCS = 15000  # для демонстрации; нужно увеличить до len(df)\n",
        "\n",
        "texts = df[\"description\"].iloc[:N_DOCS].tolist()\n",
        "\n",
        "tokenized_texts = [preprocess_text(t) for t in texts]\n",
        "\n",
        "# Смотрим пример\n",
